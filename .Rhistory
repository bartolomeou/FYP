library(truncnorm)
# Example usage
set.seed(123)
x <- rtruncnorm(100, a = 0, b = Inf, mean = 2, sd = 1)  # Generating truncated normal data
tau <- 0  # Value of threshold
# Initial guess for MLE
theta0 <- c(1, 1)
# Calculate MLE
MLE <- trnorm.MLE(x, tau, theta0)
print(MLE)
